{
  "name": "architecture-visualizer",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://architecture-visualizer.github.com/",
  "dependencies": {
    "@blueprintjs/core": "^3.47.0",
    "@blueprintjs/select": "^3.16.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "arg": "^5.0.0",
    "beautiful-react-hooks": "^1.0.2",
    "echarts": "5.1.2",
    "echarts-for-react": "^3.0.1",
    "esm": "^3.2.25",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "json-formatter-js": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "node-sensor": "node -r esm ./src/sensor/node-sensor.mjs",
    "generate-react-routers": "node -r esm ./src/sensor/generate-react-routers.mjs",
    "start": "yarn run generate-react-routers && react-scripts start",
    "build": "yarn run generate-react-routers && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.13.1",
    "yarn": "^1.22.17"
  }
}
